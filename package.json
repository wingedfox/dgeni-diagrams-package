    "angular",
    "angularjs",
    "jsdoc",
    "ngdoc",
    "docs",
    "documentation",
    "diagram",
    "flowchart",
    "sequence",
    "gantt",
    "mermaid",
    "processor",
    "generator"
  ],
  "author": "Ilya WingedFox Lebedev <ilya@lebedev.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wingedfox/dgeni-diagrams-package/issues"
  },
  "homepage": "https://github.com/wingedfox/dgeni-diagrams-package#readme",
  "dependencies": {
    "dgeni": "~0.4.2",
    "dgeni-packages": "~0.11.1",
    "lodash": "~4.5.0",
    "mermaid": "~0.5.8",
    "phantomjs": "~1.9.0",
    "q": "~1.4.1",
    "stringmap": "~0.2.2",
    "sync": "~0.2.5"
  },
  "devDependencies": {
    "coveralls": "~2.11.6",
